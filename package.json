{
  "name": "horus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write . --ignore-path .gitignore",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/lib/seed.ts",
    "db:reset": "drizzle-kit drop && npm run db:generate && npm run db:migrate && npm run db:seed",
    "woo:sync": "tsx src/lib/woo/sync.ts",
    "woo:sync-all": "tsx src/lib/woo/sync.ts --all",
    "revalidate": "tsx scripts/revalidate.ts",
    "setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@stripe/stripe-js": "^7.5.0",
    "@tanstack/react-query": "^5.83.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.4",
    "@woocommerce/woocommerce-rest-api": "^1.0.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "next-auth": "^5.0.0-beta.29",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "stripe": "^18.3.0",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5"
